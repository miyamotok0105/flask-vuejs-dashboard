{
  "name": "vue_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "postinstall": "yarn build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chartist": "0.11.0",
    "node-sass": "^7.0.0",
    "sass-loader": "7.1.0",
    "vue": "^2.5.13",
    "vue-chartist": "2.1.2",
    "vue-meta": "1.5.2",
    "vue-router": "^3.0.1",
    "vuetify": "1.4.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@mdi/font": "2.5.94",
    "@vue/cli-plugin-babel": "3.0.1",
    "@vue/cli-plugin-eslint": "3.0.1",
    "@vue/cli-service": "3.0.1",
    "@vue/eslint-config-standard": "3.0.1",
    "axios": "0.19.0",
    "lint-staged": "^6.0.0",
    "material-design-icons-iconfont": "3.0.3",
    "node-sass": "^7.0.0",
    "sass-loader": "^6.0.6",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "vue-analytics": "5.8.0",
    "vue-i18n": "7.4.0",
    "vue-router": "3.0.1",
    "vue-template-compiler": "^2.6.11",
    "vuex": "3.0.1",
    "vuex-router-sync": "5.0.0"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
